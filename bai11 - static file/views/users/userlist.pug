extends ../../template/common


block content
	a(href='/users/create') new user

	form(action='/users/search' menthod='GET')
		input(id = 'name' type='text' name='q' value = q)
		button(type='submit') Search

	ul
		each user in users
			li
				span= user.name
				span &nbsp;
				a(href='/users/' + user.id) View